{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "mocha": true
    },
    "globals": {
    },
    "rules": {
        "react/jsx-no-bind": 0,
        "arrow-body-style": 0,
        "no-case-declarations": 0,
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/forbid-prop-types": [1, { "forbid": ["any"] }],
        "camelcase": 0,
        "no-console": 0,
        "max-len": 0
    }
}